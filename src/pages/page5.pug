extends layout
block variables
	- var body_class = 'gray_bg'
block content
	section.register-section
		.container.container--register
			.reg-main.reg-main--form.reg-main--active.reg-main--visible
				h1.login-h1 Hello there!
				.login-sub-heading Tell us about yourself
				.register-block
					.register-block-top
						.register-steps-progbar
							.register-steps-progbar__finished(style="width:50%;")
						.register-steps-progtext Step 1 of 2
					form.register-steps-wrap
						.register-step.register-step--1.register-step--active.register-step--visible

							.flex-inputs
								label.input-text-wrap.input-text-wrap--gray.iw-c50
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder First Name
								label.input-text-wrap.input-text-wrap--gray.iw-c50
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder Last Name

							.flex-inputs
								label.input-text-wrap.input-text-wrap--gray.iw-c50
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder Email Address
								label.input-text-wrap.input-text-wrap--gray.iw-c50
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder Primary Phone

							.flex-inputs
								label.input-text-wrap.input-text-wrap--gray.iw-c75
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder Street Address
								label.input-text-wrap.input-text-wrap--gray.iw-c25
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder Apr / Suite #

							.flex-inputs
								label.input-text-wrap.input-text-wrap--gray.iw-c50-3
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder City
								label.iw-c25-3
									select.js-custom-select(name="State" data-placeholder="State" data-minimum-results-for-search="0")
										option(value="")
										option(value="1") State 1
										option(value="2") State 2
										option(value="3") State alabama

								label.input-text-wrap.input-text-wrap--gray.iw-c25-3
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder Zip Code

							.flex-inputs
								label.input-text-wrap.input-text-wrap--gray.iw-c50
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder Date of Birth
								.iw-c50
									select.js-custom-select(name="gender" data-placeholder="Gender")
										option(value="")
										option(value="m") Male
										option(value="f") Female




							.flex-inputs.flex-inputs-toggle
								.flex-inputs-toggle__text Do you have a Primary Care Physician?
								.flex-inputs-toggle__wrap
									label.custom-toggle2-wrap.custom-toggle2-wrap--yn
										input.custom-toggle2(type="checkbox" checked, name="")
										.custom-toggle2-active__bg
											span Yes
										.custom-toggle2-inactive__bg
											span No
										.custom-toggle2-handler

							.flex-inputs
								label.input-text-wrap.input-text-wrap--gray.iw-c50
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder Primary Care Physician Name

							.flex-inputs.flex-inputs-toggle
								.flex-inputs-toggle__text Do you have Health Insurance?
								.flex-inputs-toggle__wrap
									label.custom-toggle2-wrap.custom-toggle2-wrap--yn
										input.custom-toggle2(type="checkbox", name="")
										.custom-toggle2-active__bg
											span Yes
										.custom-toggle2-inactive__bg
											span No
										.custom-toggle2-handler

							.flex-inputs
								.iw-c50
									select.js-custom-select(name="Preferred Language" data-placeholder="Preferred Language")
										option(value="")
										option(value="Preferred Language 1") Preferred Language 1
										option(value="Preferred Language 2") Preferred Language 2

							.flex-inputs.flex-inputs--register-privacy
								.register-privacy
									label.styled-chb-wrap
										input.styled-chb.register-privacy__chb(type="checkbox", name="" checked important)
										span.styled-chb-span
									.register-privacy_text By clicking "Continue", I am providing express consent to be contacted by LiveMedConnect regarding telemedicine news, events & offers and health related tips on my wireless number using automated technology via phone call, email and SMS-Text Message. Standard message and data rates may apply and message frequency varies per user. The mobile carriers are not liable for delayed or undelivered messages. Consent is not a requirement of purchase. Review our
										a(href="")  Privacy Policy
										|  &
										a(href="")  Terms & Conditions.
							.flex-inputs.flex-inputs--continue
								btn.btn.continue-step-1 Continue
						.register-step.register-step--2
							.flex-inputs.flex-inputs--select-plan
								input.reg-selectable-plan__radio(type="radio", name="plan" id="rad-select-plan-1" checked)
								label.reg-selectable-plan(for="rad-select-plan-1")
									.reg-selectplan-top
										.reg-selectplan-top__radio-state
										.reg-selectplan-top__icon
											svg(width='24' height='24' viewbox='0 0 24 24' fill='none' xmlns='http://www.w3.org/2000/svg')
												path(d='M20 21V19C20 17.9391 19.5786 16.9217 18.8284 16.1716C18.0783 15.4214 17.0609 15 16 15H8C6.93913 15 5.92172 15.4214 5.17157 16.1716C4.42143 16.9217 4 17.9391 4 19V21' stroke='#646D86' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
												path(d='M12 11C14.2091 11 16 9.20914 16 7C16 4.79086 14.2091 3 12 3C9.79086 3 8 4.79086 8 7C8 9.20914 9.79086 11 12 11Z' stroke='#646D86' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')

										.reg-selectplan-top__plan-name Individual plan
									.reg-selectplan-price $24.95/mo
									.reg-selectplan-desc Unlimited Consultations #[br] for&nbsp;One

								input.reg-selectable-plan__radio(type="radio", name="plan" id="rad-select-plan-2")
								label.reg-selectable-plan(for="rad-select-plan-2")
									.reg-selectplan-top
										.reg-selectplan-top__radio-state
										.reg-selectplan-top__icon
											svg(width='24' height='24' viewbox='0 0 24 24' fill='none' xmlns='http://www.w3.org/2000/svg')
												path(d='M17 21V19C17 17.9391 16.5786 16.9217 15.8284 16.1716C15.0783 15.4214 14.0609 15 13 15H5C3.93913 15 2.92172 15.4214 2.17157 16.1716C1.42143 16.9217 1 17.9391 1 19V21' stroke='#646D86' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
												path(d='M9 11C11.2091 11 13 9.20914 13 7C13 4.79086 11.2091 3 9 3C6.79086 3 5 4.79086 5 7C5 9.20914 6.79086 11 9 11Z' stroke='#646D86' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
												path(d='M23 21.0001V19.0001C22.9993 18.1139 22.7044 17.2529 22.1614 16.5524C21.6184 15.852 20.8581 15.3517 20 15.1301' stroke='#646D86' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
												path(d='M16 3.13013C16.8604 3.35043 17.623 3.85083 18.1676 4.55244C18.7122 5.25405 19.0078 6.11696 19.0078 7.00513C19.0078 7.8933 18.7122 8.75621 18.1676 9.45782C17.623 10.1594 16.8604 10.6598 16 10.8801' stroke='#646D86' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')

										.reg-selectplan-top__plan-name Family plan
									.reg-selectplan-price
										span.reg-selectplan-oldprice.dm $39.95
										span  $29.95/mo
									.reg-selectplan-desc Consultations for Family #[br] up to 5 Members
							.center-toggle-wrap
								.center-toggle-text Credit Card
								label.custom-toggle2-wrap.custom-toggle2-wrap.center-toggle-toggler
									input.custom-toggle2(type="checkbox", name="")
									.custom-toggle2-handler
								.center-toggle-text PayPal
							.flex-inputs
								label.input-text-wrap.input-text-wrap--gray.iw-c50-3
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder Card Number

								label.input-text-wrap.input-text-wrap--gray.iw-c25-3
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder Expiration

								label.input-text-wrap.input-text-wrap--gray.iw-c25-3
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder CVC

							.flex-inputs
								label.input-text-wrap.input-text-wrap--gray.iw-c50
									input.textinp.textinp--no-pic.textinp--gray(required type="text", name="")
									span.textinp-top-placeholder Cardholderâ€™s Name
								.iw-c50.flex-inputs.flex-inputs--aic
									.flex-inputs-toggle__text Same billing address?
									.flex-inputs-toggle__wrap
										label.custom-toggle2-wrap.custom-toggle2-wrap--yn
											input.custom-toggle2(type="checkbox" checked, name="")
											.custom-toggle2-active__bg
												span Yes
											.custom-toggle2-inactive__bg
												span No
											.custom-toggle2-handler

							.flex-inputs
								.register-privacy
									label.styled-chb-wrap
										input.styled-chb.register-privacy__chb(type="checkbox", name="" checked important)
										span.styled-chb-span
									.register-privacy_text-2 I have read and agree to the #[a(href="")  Terms and Conditions], and I am aware that I will be billed $24.95 each month until I request cancellation of services.
									.btn.continue-step-2 Continue

				.login-create-acc
					span Already have an account?
					a.login-create-acc__link(href="")  Log in


			.reg-main.reg-main--thanks
				.reg-thank-pic
					img(src="new/images/login-pic.png", alt="")
				.reg-thank-title Thank you!
				.reg-thank-text
					p Weâ€™ve sent email on
						a(href="mailto:johnsmith@example.com")  johnsmith@example.com
						| .
					p Please check your inbox.
				.reg-thank-sub If you have any questions, please contact us
					| at +1 (833) #[a(href="") 4Live-MD] or #[a(href="") email]  us.